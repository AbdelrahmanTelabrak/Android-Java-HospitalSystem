        android:id="@+id/reportsListsFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Reports.ReportsListsFragment"
        android:label="ReportsListsFragment"
        tools:layout="@layout/fragment_reports_lists">
        <action
            android:id="@+id/action_reportsListsFragment_to_createReportFragment"
            app:destination="@id/createReportFragment" />
    </fragment>
    <fragment
        android:id="@+id/createReportFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Reports.CreateReportFragment"
        android:label="CreateReportFragment"
        tools:layout="@layout/fragment_create_report">
        <action
            android:id="@+id/action_createReportFragment_to_reportsListsFragment"
            app:destination="@id/reportsListsFragment" />
    </fragment>
    <fragment
        android:id="@+id/managerCreateCallFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Manager.ManagerCreateCallFragment"
        android:label="ManagerCreateCallFragment"
        tools:layout="@layout/fragment_create_call_manager"/>
    <fragment
        android:id="@+id/docCasesListFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Doctor.DocCasesListFragment"
        android:label="DocCasesListFragment"
        tools:layout="@layout/fragment_doc_cases_list">
        <action
            android:id="@+id/action_docCasesListFragment_to_docCaseDFragment"
            app:destination="@id/docCaseDFragment" />
        <action
            android:id="@+id/action_docCasesListFragment_to_doctorMenuFragment"
            app:destination="@id/doctorMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/docCaseDFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Doctor.DocCaseDFragment"
        android:label="DocCaseDFragment"
        tools:layout="@layout/fragment_doc_case_d">
        <action
            android:id="@+id/action_docCaseDFragment_to_docCasesListFragment"
            app:destination="@id/docCasesListFragment" />
        <action
            android:id="@+id/action_docCaseDFragment_to_docSelectNurseFragment"
            app:destination="@id/docSelectNurseFragment" />
        <argument
            android:name="caseId"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/docSelectNurseFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Doctor.DocSelectNurseFragment"
        android:label="DocSelectNurseFragment"
        tools:layout="@layout/fragment_doc_select_nurse">
        <action
            android:id="@+id/action_docSelectNurseFragment_to_docCaseDFragment"
            app:destination="@id/docCaseDFragment" />
        <argument
            android:name="caseId"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.StartUp.SplashScreenFragment"
        android:label="SplashScreenFragment"
        tools:layout="@layout/fragment_splash_screen">
        <action
            android:id="@+id/action_splashScreenFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/main_nav" />
        <action
            android:id="@+id/action_splashScreenFragment_to_doctorMenuFragment"
            app:destination="@id/doctorMenuFragment"
            app:popUpTo="@id/main_nav" />
        <action
            android:id="@+id/action_splashScreenFragment_to_nurseMenuFragment"
            app:destination="@id/nurseMenuFragment"
            app:popUpTo="@id/main_nav" />
        <action
            android:id="@+id/action_splashScreenFragment_to_receptionistMenuFragment"
            app:destination="@id/receptionistMenuFragment"
            app:popUpTo="@id/main_nav" />
        <action
            android:id="@+id/action_splashScreenFragment_to_analysisEmpMenuFragment"
            app:destination="@id/analysisEmpMenuFragment"
            app:popUpTo="@id/main_nav" />
        <action
            android:id="@+id/action_splashScreenFragment_to_managerMenuFragment"
            app:destination="@id/managerMenuFragment"
            app:popUpTo="@id/main_nav" />
        <action
            android:id="@+id/action_splashScreenFragment_to_mainMenuHrFragment"
            app:destination="@id/mainMenuHrFragment"
            app:popUpTo="@id/main_nav" />
    </fragment>
    <fragment
        android:id="@+id/managerTasksFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Manager.ManagerTasksFragment"
        android:label="ManagerTasksFragment"
        tools:layout="@layout/fragment_manager_tasks">
        <action
            android:id="@+id/action_managerTasksFragment_to_managerMenuFragment"
            app:destination="@id/managerMenuFragment" />
        <action
            android:id="@+id/action_managerTasksFragment_to_managerCreateTaskFragment"
            app:destination="@id/managerCreateTaskFragment" />
    </fragment>
    <dialog
        android:id="@+id/toDoBottomSheetFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Manager.ToDoBottomSheetFragment"
        android:label="ToDoBottomSheetFragment"
        tools:layout="@layout/fragment_to_do_bottom_sheet">
        <action
            android:id="@+id/action_toDoBottomSheetFragment_to_managerCreateTaskFragment"
            app:destination="@id/managerCreateTaskFragment" />
    </dialog>
    <fragment
        android:id="@+id/managerCreateTaskFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Manager.ManagerCreateTaskFragment"
        android:label="ManagerCreateTaskFragment"
        tools:layout="@layout/fragment_manager_create_task">
        <action
            android:id="@+id/action_managerCreateTaskFragment_to_toDoBottomSheetFragment"
            app:destination="@id/toDoBottomSheetFragment" />
        <argument
            android:name="toDo"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="null" />
        <action
            android:id="@+id/action_managerCreateTaskFragment_to_managerSelectEmpFragment"
            app:destination="@id/managerSelectEmpFragment" />
        <argument
            android:name="empIdName"
            app:argType="com.example.hospitalsystem_abdelrahmantarek.Models.DocNameId"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_managerCreateTaskFragment_to_managerTasksFragment"
            app:destination="@id/managerTasksFragment" />
    </fragment>
    <fragment
        android:id="@+id/managerSelectEmpFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Manager.ManagerSelectEmpFragment"
        android:label="ManagerSelectEmpFragment"
        tools:layout="@layout/fragment_manager_select_emp">
        <action
            android:id="@+id/action_managerSelectEmpFragment_to_managerCreateTaskFragment"
            app:destination="@id/managerCreateTaskFragment" />
    </fragment>
    <fragment
        android:id="@+id/tasksListFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Tasks.TasksListFragment"
        android:label="TasksListFragment" />
