        tools:layout="@layout/fragment_case_d_progress">
        <action
            android:id="@+id/action_caseDProgressFragment_to_caseDFinishedFragment"
            app:destination="@id/caseDFinishedFragment" />
    </fragment>
    <fragment
        android:id="@+id/caseDFinishedFragment"
        android:name="com.example.hospitalsystem_abdelrahmantarek.Receptionist.CaseDFinishedFragment"
        android:label="CaseDFinishedFragment"
        tools:layout="@layout/fragment_case_d_finished"/>
